{
  "name": "mern-todo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.20.1",
    "bootstrap": "^4.1.3",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.6.6",
    "npm-run-all": "^4.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "yarn": "^1.16.0"
  },
  "devDependencies": {
    "dotenv": "^8.0.0",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "mongoStart": "cd backend && nohup mongod >> /mongo.log",
    "mongoConsole": "cd backend && mongo",
    "serverStart": "cd backend && nodemon server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "runApp": "concurrently \"pm2 start npm --run start\" \"npm run mongoStart\" \"npm run mongoConsole\" \"pm2 start npm --run serverStart\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
